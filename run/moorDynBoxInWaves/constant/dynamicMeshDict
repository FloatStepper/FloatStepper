FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh   dynamicFloaterMotionSolversFvMesh;

addedMassModel      pisoAddedMass;

solvers
{
    box
    {
        motionSolverLibs ( floaterMotion );

        motionSolver    floaterMotion;

        floaterMotionCoeffs
        {
            patches         (box);
            innerDistance   0.075;
            outerDistance   0.4;

            centreOfMass    (0 0 0); // Relative to CoR in floaterMotionState
            mass            3.148;
            momentOfInertia (0.015 0 0 0.0167 0 0.021); // Wrt CoM
            rotDirs   (0 1 0);
            linDirs   (1 0 1);
            MaddUpdateFreq  2;

            restraints
            {
                moorDynR1
                {
                    floaterMotionRestraint moorDynR1;
                }
            }
        }
    }
}


