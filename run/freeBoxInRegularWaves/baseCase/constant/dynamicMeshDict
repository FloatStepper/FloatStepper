FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          dynamicMeshDict;
}

dynamicFvMesh   dynamicPointDisplacementFvMesh;

solvers
{
    myBox
    {
        motionSolverLibs ( floaterMotion );
        motionSolver    floaterMeshMotionSolver;
        floaterMeshMotionSolverCoeffs
        {
            patches         ( box );
            innerDistance   0.02;
            outerDistance   1.5;
            distStretch     ( 1 0 0 0 1 0 0 0 5 );
            linDirs         ( 1 0 1 );
            rotDirs         ( 0 1 0 );
            MaddUpdateFreq  1;
            addedMassModel  pisoAddedMass;
            restraints
            {
            }
        }
    }
}


