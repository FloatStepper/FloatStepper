FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          dynamicMeshDict;
}

dynamicFvMesh       dynamicMotionSolverListFvMesh;

solvers
{
    circle
    {

        motionSolverLibs ( floaterMotion );
        motionSolver    floaterMotion;

        floaterMotionCoeffs
        {
            addedMassModel  pisoAddedMass;
            patches         ( floatingObject );
            innerDistance   1;
            outerDistance   9.5;
            linDirs         ( 0 1 0 );
            rotDirs         ( 0 0 0 );
            MaddUpdateFreq  1;

            restraints
            {
            }
        }
    }
}

