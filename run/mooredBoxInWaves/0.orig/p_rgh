FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedFluxExtrapolatedPressure;
        value           uniform 0;
    }
    outlet
    {
        type            fixedFluxExtrapolatedPressure;
        value           uniform 0;
    }
    stationaryWalls
    {
        type            fixedFluxExtrapolatedPressure;
    }
    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
    }
    floatingObject
    {
        type            fixedFluxExtrapolatedPressure;
    }
}

// ************************************************************************* //
