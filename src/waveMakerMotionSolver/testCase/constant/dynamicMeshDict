FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh   dynamicPointDisplacementFvMesh;

motionSolverLibs    ( libfloaterMotion );

solvers
{
    waveMaker
    {

        motionSolverLibs ( waveMakerMotionSolver );

        motionSolver waveMakerMotionSolver;

        times 5 ( 0 1 2 3 4 );

        pistonPositions 1 ( 5 ( 0 1 0 -1 0 ) );

        zPositions ( 0 1 );
        zScaling ( 0 1 );

        xLeft	0.5;
        xRight	3;

