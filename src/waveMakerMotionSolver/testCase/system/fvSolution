FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "alpha.*"
    {
        isoFaceTol      1e-10;
        surfCellTol     1e-10;
        nAlphaBounds    5;
        snapTol         0;
        clip            false;
        cAlpha          1; //Read by interIsoFoam but not used
        nAlphaSubCycles 1;
        reconstructionScheme plicRDF; // isoAlpha
    }
}


PIMPLE
{
    frozenFlow      yes;
    momentumPredictor no;
    nCorrectors     -1;
    nNonOrthogonalCorrectors -1;
    pRefCell        0;
    pRefValue       0;
}


