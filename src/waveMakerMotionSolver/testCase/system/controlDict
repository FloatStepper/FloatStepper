FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     moveDynamicMesh;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         9;

writeControl    timeStep;

writeInterval   1;

deltaT          0.1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression off;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable no;

adjustTimeStep  no;

maxCo           1e6;

maxAlphaCo      0.5;

maxDeltaT       1e6;

libs ( libfloaterMotion );

