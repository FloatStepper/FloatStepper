FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type           fixedValue;
        value          uniform (0 0 0);
    }
    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            fixedNormalSlip;
        n               (0 0 1);
    }
    frontAndBack
    {
        type            empty;
    }
    atmosphere
    {
        type            calculated;
        n               (0 0 1);
    }
    floatingObject
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    floatingObject2
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
