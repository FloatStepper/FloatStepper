// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "0/uniform";
    object          floaters;
}

box
{
    #include "inertia1"

    centreOfRotation    $centreOfMass;

    orientation         $principleAxes;

    velocity            ( 0 0 0 );

    acceleration        ( 0 0 0 );

    angularVelocity     ( 0 0 0 );

    angularAcceleration ( 0 0 0 );

    initialCentreOfRotation $centreOfRotation;

    initialOrientation  $orientation;

    initialCentreOfMass $centreOfMass;
}


box2
{
    #include "inertia2"

    centreOfRotation    $centreOfMass;

    orientation         $principleAxes;

    velocity            ( 0 0 0 );

    acceleration        ( 0 0 0 );

    angularVelocity     ( 0 0 0 );

    angularAcceleration ( 0 0 0 );

    initialCentreOfRotation $centreOfRotation;

    initialOrientation  $orientation;

    initialCentreOfMass $centreOfMass;
}

