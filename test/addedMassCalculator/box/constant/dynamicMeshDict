FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          dynamicMeshDict;
}

dynamicFvMesh   staticFvMesh;

solvers
{
    body
    {
        motionSolverLibs ( floaterMotion );
        motionSolver    floaterMotion;
        floaterMotionCoeffs
        {
            addedMassModel  potentialAddedMass;
            patches         ( floatingObject );
            linDirs         ( 1 0 1 );
            rotDirs         ( 0 1 0 );
        }
    }
}


