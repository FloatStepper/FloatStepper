FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "0/uniform";
    object          floaters;
}

body
{

    centreOfRotation    ( 0 1 0 );

    orientation         ( 1 0 0 0 1 0 0 0 1 );

    velocity            ( 1 1 0 );

    angularVelocity     ( 0 0 1 );
}

