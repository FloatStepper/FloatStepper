FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     test-pisoAddedMass;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime	        10;

deltaT	        0.005;

writeControl    adjustable;

writeInterval   0.1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression no;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           .5;
maxAlphaCo      .5;

maxDeltaT       1;

// Needed by interIsoFoam_reconPhi to get the sixDoFRigidBodySlipVelocity BC
libs (addedMass);

